{
	"manifest_version": 2,
	"name": "BuyMe",
	"description": "A chrome extension that helps you find cool stuff",
	"version": "1.0",

	 "chrome_url_overrides": {
    "newtab": "popup.html"
  },

	"permissions": ["https://*.google.com/"],
  	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

	"icons": {
		"icon": "icon.png"
	},
	"browser_action": {
		"default_icon": {
			"icon": "icon.png"
		},
		"default_popup": "popup.html"
	}


}
